{
  "training": {
    "optimizer": "adam",
    "learning_rate": 0.0003,
    "max_norm": 3.0,
    "epochs": 10000,
    "batches_per_report": 100,
    "batches_per_sampling": 20,
    "random_seed": 1
  },
  "data": {
    "src": "data/Ustance/pos.train.txt",
    "tgt": "data/Ustance/neg.train.txt",
    "src_test": "data/Ustance/pos.test.txt",
    "tgt_test": "data/Ustance/neg.test.txt",
    "src_vocab": "data/Ustance/vocab",
    "tgt_vocab": "data/Ustance/vocab",
    "share_vocab": true,
    "attribute_vocab": "data/Ustance/ngram.15.attribute",
    "ngram_attributes": true,
    "batch_size": 20,
    "max_len": 50,
    "working_dir": "working_dir"
  },
    "model": {
        "model_type": "delete_retrieve",
        "emb_dim": 128,
        "attention": false,
        "encoder": "lstm",
        "src_hidden_dim": 512,
        "src_layers": 1,
        "bidirectional": true,
        "tgt_hidden_dim": 512,
        "tgt_layers": 1,
        "decode": "greedy",
        "dropout": 0.2
    }
}